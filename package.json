    "build-once": "tsc",
    "test": "ava */tests/test_*.js --require ./common/_ensure_rejected_handled",
    "typings": "typings"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markfinger/unfort.git"
  },
