    "babel-core": "^6.10.4",
    "babel-generator": "^6.11.0",
    "babel-traverse": "^6.10.4",
    "babel-types": "^6.11.1",
    "babylon": "^6.8.4",
    "bluebird": "^3.4.1",
    "browser-resolve": "^1.11.2",
    "browserify": "^13.1.0",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.0",
    "css-selector-tokenizer": "^0.7.0",
