{
  "name": "unfort",
  "version": "1.3.0",
  "description": "A build tool for the web that prioritises performance during development. Fundamentally, this project is a greenfield reimplementation of a subset of webpack's features.",
  "main": "lib/unfort.js",
  "scripts": {
    "build": "./scripts/build.js",
    "prepublish": "npm run build -- --once",
    "test": "mocha --require source-map-support/register --reporter spec 'lib/**/tests/*.js'",
    "_test": "mocha --reporter spec 'lib/**/tests/*.js'",
    "lint": "eslint runtimes/*.js scripts/**.js src/**.js",
    "coverage": "babel-node node_modules/babel-istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- --reporter spec 'src/**/tests/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markfinger/unfort.git"
  },
  "author": "Mark Finger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markfinger/unfort/issues"
  },
  "homepage": "https://github.com/markfinger/unfort",
  "dependencies": {
    "babel-code-frame": "^6.5.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-generator": "^6.8.0",
    "babylon": "^6.8.0",
    "babylon-ast-dependencies": "^1.0.1",
    "browser-resolve": "^1.11.1",
    "browserify": "^13.0.1",
    "chalk": "^1.1.1",
    "chokidar": "^1.4.2",
    "css-selector-tokenizer": "^0.5.4",
    "cyclic-dependency-graph": "^1.0.0",
    "env-hash": "^1.0.0",
    "immutable": "^3.7.6",
    "imurmurhash": "^0.1.4",
    "kv-cache": "^1.1.0",
    "lodash": "^4.12.0",
    "mime-types": "^2.1.11",
    "mkdirp": "^0.5.1",
    "postcss": "^5.0.21",
    "postcss-ast-dependencies": "^1.1.0",
    "promisify-node": "^0.4.0",
    "record-store": "^1.0.0",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.4.0",
    "strip-ansi": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-istanbul": "^0.11.0",
    "babel-plugin-check-es2015-constants": "^6.8.0",
    "babel-plugin-transform-es2015-destructuring": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-plugin-transform-es2015-parameters": "^6.8.0",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.5.0",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.2.0",
    "chai-immutable": "^1.5.3",
    "codecov": "^1.0.1",
    "eslint": "^2.9.0",
    "istanbul": "^0.4.2",
    "memorystream": "^0.3.1",
    "mocha": "^2.2.5",
    "rimraf": "^2.5.0",
    "yargs": "^4.7.0"
  }
}
